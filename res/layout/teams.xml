<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#0000CC"
    android:orientation="vertical">

    <TextView 
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:text="Mannschaften"
        android:gravity="center"
        android:textColor="#FFD700"
        android:textSize="30dip"
        android:textStyle="bold" 
        android:layout_weight="50"/>
    
	<View
		android:layout_height="3dip"
		android:layout_width="fill_parent"
		android:background="#000000"
		android:layout_marginBottom="5dip" 
		android:layout_marginTop="5dip"/>
	
    <LinearLayout 
        android:layout_width="fill_parent"
   		android:layout_height="fill_parent"
    	android:orientation="horizontal" 
    	android:layout_weight="50">
		<Button 
		    android:id="@+id/b1"
		    android:layout_width="fill_parent"
		    android:layout_height="fill_parent"
		    android:text="1"
		    android:textStyle="bold"            
		    android:textColor="#FFD700"
		    android:textSize="20dip"
			android:background="@drawable/border"
		    android:layout_weight="50"
		    android:layout_margin="2dip"/>
		<Button 
		    android:id="@+id/b2"
		    android:layout_width="fill_parent"
		    android:layout_height="fill_parent"
		    android:text="2" 
		    android:textStyle="bold"            
		    android:textColor="#FFD700"
		    android:textSize="20dip"
		    android:background="@drawable/border"
		    android:layout_margin="2dip"
		    android:layout_weight="50"/>
		<Button 
		    android:id="@+id/b3"
		    android:layout_width="fill_parent"
		    android:textColor="#FFD700"
		    android:textSize="20dip"
		    android:textStyle="bold"            
		    android:background="@drawable/border"		    
		    android:layout_height="fill_parent"
		    android:text="3" 
		    android:layout_margin="2dip"
		    android:layout_weight="50"/>
	</LinearLayout>

  	<View
		android:layout_height="3dip"
		android:layout_width="fill_parent"
		android:background="#000000"
		android:layout_marginBottom="5dip" 
		android:layout_marginTop="5dip"/>	
		
    <LinearLayout 
        android:layout_width="fill_parent"
   		android:layout_height="fill_parent"
    	android:orientation="horizontal"
    	android:layout_weight="50">		
		<Button 
		    android:id="@+id/bNachwuchs"
		    android:layout_width="fill_parent"
		    android:layout_height="fill_parent"
		    android:text="Nachwuchs"
		    android:textColor="#FFD700"
		    android:textStyle="bold"            
		    android:textSize="20dip"
		    android:layout_margin="2dip"
		    android:background="@drawable/border"		    
		    android:layout_weight="50"/>
		<Button 
		    android:id="@+id/bSenioren"
		    android:layout_width="fill_parent"
		    android:layout_height="fill_parent"
		    android:textColor="#FFD700"
		    android:textSize="20dip"
		    android:textStyle="bold"    
		    android:layout_margin="2dip"        
		    android:background="@drawable/border"		    
		    android:text="Senioren"
		    android:layout_weight="50"/>
		<Button 
		    android:id="@+id/bVeteranen"
		    android:layout_width="fill_parent"
		    android:layout_height="fill_parent"
		    android:textColor="#FFD700"
		    android:textStyle="bold"            
		    android:textSize="20dip"
		    android:background="@drawable/border"		    
		    android:text="Veteranen"
		    android:layout_margin="2dip"
		    android:layout_weight="50"/>
	</LinearLayout>
	
	<View
		android:layout_height="3dip"
		android:layout_width="fill_parent"
		android:background="#000000"
		android:layout_marginBottom="5dip" 
		android:layout_marginTop="5dip"/>	
		
    <LinearLayout 
        android:layout_width="fill_parent"
   		android:layout_height="fill_parent"
    	android:orientation="horizontal" android:gravity="center" android:layout_weight="50">

        <Button
            android:id="@+id/bFrauen"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:gravity="center"
		    android:textColor="#FFD700"
		    android:textStyle="bold"    
		    android:textSize="20dip"
		    android:background="@drawable/border"            
            android:text="Frauen" android:layout_weight="1"/>

	</LinearLayout>
	
	<View
		android:layout_height="3dip"
		android:layout_width="fill_parent"
		android:background="#000000"
		android:layout_marginBottom="5dip" 
		android:layout_marginTop="5dip"/>	
			
    <LinearLayout 
        android:layout_width="fill_parent"
   		android:layout_height="fill_parent"
    	android:orientation="horizontal" android:layout_weight="50">	
		<Button 
		    android:id="@+id/bBa"
		    android:layout_width="fill_parent"
		    android:layout_height="fill_parent"
		    android:text="Ba"
		    android:textColor="#FFD700"
		    android:textStyle="bold"            
		    android:textSize="20dip"
		    android:layout_margin="2dip"
		    android:background="@drawable/border"
		    android:layout_weight="50"/>
		<Button 
		    android:id="@+id/bBb"
		    android:layout_width="fill_parent"
		    android:layout_height="fill_parent"
		    android:text="Bb"
		    android:textColor="#FFD700"
		    android:textStyle="bold"            
		    android:textSize="20dip"
		    android:layout_margin="2dip"
		    android:background="@drawable/border"		    
		    android:layout_weight="50"/>
		<Button 
		    android:id="@+id/bCa"
		    android:layout_width="fill_parent"
		    android:layout_height="fill_parent"
		    android:text="Ca"
		    android:textColor="#FFD700"
		    android:textStyle="bold"            
		    android:textSize="20dip"
		    android:layout_margin="2dip"
		    android:background="@drawable/border"		    
		    android:layout_weight="50"/>
	</LinearLayout>	

	<View
		android:layout_height="3dip"
		android:layout_width="fill_parent"
		android:background="#000000"
		android:layout_marginBottom="5dip" 
		android:layout_marginTop="5dip"/>	
		    	
    <LinearLayout 
        android:layout_width="fill_parent"
   		android:layout_height="fill_parent"
    	android:orientation="horizontal" android:layout_weight="50">		
		<Button 
		    android:id="@+id/bDa"
		    android:layout_width="fill_parent"
		    android:layout_height="fill_parent"
		    android:text="Da"
		    android:textColor="#FFD700"
		    android:textStyle="bold"    
		    android:layout_margin="2dip"        
		    android:textSize="20dip"
		    android:background="@drawable/border"		    
		    android:layout_weight="50"/>
		<Button 
		    android:id="@+id/bDb"
		    android:layout_width="fill_parent"
		    android:layout_height="fill_parent"
		    android:text="Db"
		    android:textColor="#FFD700"
		    android:textStyle="bold"            
		    android:textSize="20dip"
		    android:layout_margin="2dip"
		    android:background="@drawable/border"		    
		    android:layout_weight="50"/>
		<Button 
		    android:id="@+id/bDc"
		    android:layout_width="fill_parent"
		    android:layout_height="fill_parent"
		    android:text="Dc"
		    android:textColor="#FFD700"
		    android:textStyle="bold"    
		    android:layout_margin="2dip"        
		    android:textSize="20dip"
		    android:background="@drawable/border"		    
		    android:layout_weight="50"/>
	</LinearLayout>
	
	<View
		android:layout_height="3dip"
		android:layout_width="fill_parent"
		android:background="#000000"
		android:layout_marginBottom="5dip" 
		android:layout_marginTop="5dip"/>	
		    
    <LinearLayout 
        android:layout_width="fill_parent"
   		android:layout_height="fill_parent"
    	android:orientation="horizontal" android:layout_weight="50">		
		<Button 
		    android:id="@+id/bEa"
		    android:layout_width="fill_parent"
		    android:layout_height="fill_parent"
		    android:text="Ea"
		    android:textColor="#FFD700"
		    android:textStyle="bold"    
		    android:layout_margin="2dip"        
		    android:textSize="20dip"
		    android:background="@drawable/border"		    
		    android:layout_weight="50"/>
		<Button 
		    android:id="@+id/bEb"
		    android:layout_width="fill_parent"
		    android:layout_height="fill_parent"
		    android:text="Eb"
		    android:textColor="#FFD700"
		    android:textSize="20dip"
		    android:textStyle="bold"    
		    android:layout_margin="2dip"        
		    android:background="@drawable/border"		    
		    android:layout_weight="50"/>
		<Button 
		    android:id="@+id/bEc"
		    android:layout_width="fill_parent"
		    android:layout_height="fill_parent"
		    android:text="Ec"
		    android:textColor="#FFD700"
		    android:textSize="20dip"
		    android:layout_margin="2dip"
		    android:background="@drawable/border"	
		    android:textStyle="bold"            		    	    
		    android:layout_weight="50"/>
	</LinearLayout>		
	
	<View
		android:layout_height="3dip"
		android:layout_width="fill_parent"
		android:background="#000000"
		android:layout_marginBottom="5dip" 
		android:layout_marginTop="5dip"/>
		
    <LinearLayout 
        android:layout_width="fill_parent"
   		android:layout_height="fill_parent"
    	android:orientation="horizontal" android:layout_weight="50">		
		<Button 
		    android:id="@+id/bFa"
		    android:layout_width="fill_parent"
		    android:layout_height="fill_parent"
		    android:text="Fa"
		    android:textColor="#FFD700"
		    android:textStyle="bold"    
		    android:layout_margin="2dip"        
		    android:textSize="20dip"
		    android:background="@drawable/border"		    
		    android:layout_weight="50"/>
		<Button 
		    android:id="@+id/bFb"
		    android:layout_width="fill_parent"
		    android:layout_height="fill_parent"
		    android:text="Fb"
		    android:textColor="#FFD700"
		    android:textSize="20dip"
		    android:textStyle="bold"    
		    android:layout_margin="2dip"        
		    android:background="@drawable/border"		    
		    android:layout_weight="50"/>
		<Button 
		    android:id="@+id/bPiccolos"
		    android:layout_width="fill_parent"
		    android:layout_height="fill_parent"
		    android:text="Piccolos"
		    android:textColor="#FFD700"
		    android:textSize="20dip"
		    android:layout_margin="2dip"
		    android:background="@drawable/border"	
		    android:textStyle="bold"            		    	    
		    android:layout_weight="50"/>
	</LinearLayout>		

	
</LinearLayout>